@model PagedList.IPagedList<TouristAgency.Domain.Models.EfModels.TypesTour>
@using PagedList.Mvc;
<link href="~/Content/PagedList.css" rel="stylesheet" type="text/css" />
<link rel="stylesheet" href="~/Content/MyStyle.css">
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{
    ViewBag.Title = "Index";
}

<head>
    <title>Index</title>
</head>
<h2>Index</h2>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Тип тура")
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TypeTour)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.IdTypeTour }) |
                @Html.ActionLink("Details", "Details", new { id = item.IdTypeTour })
            </td>
        </tr>
    }
</table>
 Страница @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) из @Model.PageCount
<div class="container col-4">
    @Html.PagedListPager(Model, page => Url.Action("Index", new { page }))
</div>